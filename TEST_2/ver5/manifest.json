{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ui.demo.db",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
  	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.demo.db.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.db.i18n.i18n"
				}
			},
			"people": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "https://services.odata.org/TripPinRESTierService/(S(pbifwq4mabylls1sbsykc25r))/People"
			}
			},
            "routing": {
               "config": {
                  "routerClass": "sap.m.routing.Router",
                  "viewType": "XML",
                  "viewPath": "sap.ui.demo.db.view",
                  "controlId": "app",
                  "controlAggregation": "pages",
                  "transition": "slide",
                  "async": true
               },
               "routes": [{
                  "pattern": "",
                  "name": "appHome",
						"target": "home"
					},
					{
					"pattern": "detail",
					"name": "detail",
					"target": "detail"
					},		
					{
						"pattern": "detail/{invoicePath}",
						"name": "detail",
						"target": "detail"
					 }
],
               "targets": {
                  "home": {
                     "viewId": "home",
                     "viewName": "App",
                     "viewLevel" : 1
                  },
						"detail": {
						"viewId": "detail",
						"viewName": "Detail",
						"viewPath": "sap.ui.demo.db.view",
						"transition": "show",
						"viewLevel" : 2
					}
               }
            }
		}
}
